<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>user servlet</servlet-name>
    <servlet-class>servlets.UserServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>user servlet</servlet-name>
    <url-pattern>/user</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>apartment owner servlet</servlet-name>
    <servlet-class>servlets.ApartmentOwnersServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>apartment owner servlet</servlet-name>
    <url-pattern>/apartmentOwner</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>apartment</servlet-name>
    <servlet-class>servlets.ApartmentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>apartment</servlet-name>
    <url-pattern>/apartment</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>rent servlet</servlet-name>
    <servlet-class>servlets.RentApartmentServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>rent servlet</servlet-name>
    <url-pattern>/rent</url-pattern>
  </servlet-mapping>
  


</web-app>
